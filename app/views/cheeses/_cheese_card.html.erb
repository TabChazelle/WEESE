<%= link_to cheese_path(cheese), class: "cheese-card-link" do %>
<div class="cheese-card">
  <div class="image-container">
    <%= image_tag cheese.image_url, alt: cheese.name %>
  </div>
  <h3><%= cheese.name %></h3>
  <h6><%= cheese.region %></h6>
  <h6><%= cheese.country %></h6>
  <%# <p><%= truncate(cheese.description, length: 100) </p> %>
</div>
<% end %>


